{
  "name": "solid-phosphor",
  "version": "1.0.5",
  "type": "module",
  "description": "Phosphor icons for SolidJS",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "tsc",
    "clean": "tsx ./bin/clean.ts",
    "generate": "tsx ./bin/generate.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.12.12",
    "solid-js": "^1.8.17",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1"
  },
  "peerDependencies": {
    "solid-js": "^1.7.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./bold*": {
      "import": "./dist/icons/bold/*.js",
      "types": "./dist/icons/bold/*.d.ts"
    },
    "./duotone*": {
      "import": "./dist/icons/duotone/*.js",
      "types": "./dist/icons/duotone/*.d.ts"
    },
    "./fill*": {
      "import": "./dist/icons/fill/*.js",
      "types": "./dist/icons/fill/*.d.ts"
    },
    "./light*": {
      "import": "./dist/icons/light/*.js",
      "types": "./dist/icons/light/*.d.ts"
    },
    "./regular*": {
      "import": "./dist/icons/regular/*.js",
      "types": "./dist/icons/regular/*.d.ts"
    },
    "./thin*": {
      "import": "./dist/icons/thin/*.js",
      "types": "./dist/icons/thin/*.d.ts"
    }
  }
}