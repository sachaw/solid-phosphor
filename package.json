{
  "name": "solid-phosphor",
  "version": "1.0.2",
  "type": "module",
  "description": "Phosphor icons for SolidJS",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "tsc",
    "clean": "tsx ./bin/clean.ts",
    "generate": "tsx ./bin/generate.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "biome": "^0.3.3",
    "solid-js": "^1.8.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@phosphor-icons/core": "^2.0.3"
  },
  "peerDependencies": {
    "solid-js": "^1.7.3"
  },
  "files": ["dist", ".npmrc"],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./bold": {
      "import": {
        "types": "./dist/icons/bold/index.d.ts",
        "default": "./dist/icons/bold/index.js"
      }
    },
    "./duotone": {
      "import": {
        "types": "./dist/icons/duotone/index.d.ts",
        "default": "./dist/icons/duotone/index.js"
      }
    },
    "./fill": {
      "import": {
        "types": "./dist/icons/fill/index.d.ts",
        "default": "./dist/icons/fill/index.js"
      }
    },
    "./light": {
      "import": {
        "types": "./dist/icons/light/index.d.ts",
        "default": "./dist/icons/light/index.js"
      }
    },
    "./regular": {
      "import": {
        "types": "./dist/icons/regular/index.d.ts",
        "default": "./dist/icons/regular/index.js"
      }
    },
    "./thin": {
      "import": {
        "types": "./dist/icons/thin/index.d.ts",
        "default": "./dist/icons/thin/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
