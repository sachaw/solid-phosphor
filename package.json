{
  "name": "solid-phosphor",
  "version": "1.0.7",
  "type": "module",
  "description": "Phosphor icons for SolidJS",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "tsc",
    "clean": "tsx ./bin/clean.ts",
    "generate": "tsx ./bin/generate.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.12.12",
    "solid-js": "^1.8.17",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@phosphor-icons/core": "^2.1.1"
  },
  "peerDependencies": {
    "solid-js": "^1.7.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./*": {
      "import": "./dist/src/icons/*.js",
      "types": "./dist/src/icons/*.d.ts"
    }
  }
}